var names = ["jack", "cbill", "steve", "bandrew", "karrot", "thomas","ally"]
text = "";
for (j = 0; j < names.length; j++) {
    for (i = 0; i < names.length - 1; i++) {
        text += names[i] + "<br/>";
        var first = names[i];
        var second = names[i + 1];
        console.log(names);
        if (first.substring(0, 1) > second.substring(0, 1)) {
            t = names[i];
            names[i] = names[i + 1];
            names[i + 1] = t;
        }

    }
}
